{
  "name": "mithril-objectify",
  "version": "1.0.0",
  "description": "Optimize Mithril m() calls into simple JS objects via API, CLI, or Browserify",
  "main": "index.js",
  "bin": "./bin/cli.js",
  "scripts": {
    "test": "tape test/*.js"
  },
  "author": "Pat Cavit <pcavit@arena.net>",
  "repository": "tivac/mithril-objectify",
  "license": "MIT",
  "dependencies": {
    "falafel": "^1.2.0",
    "through2": "^2.0.0"
  },
  "devDependencies": {
    "tape": "^4.2.0"
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "templateStrings": true
    },
    "env": {
      "node": true
    },
    "rules": {
      "curly": 2,
      "dot-notation": 2,
      "eol-last": 0,
      "no-caller": 2,
      "no-constant-condition": 2,
      "no-dupe-args": 2,
      "no-dupe-keys": 2,
      "no-duplicate-case": 2,
      "no-empty": 2,
      "no-eval": 2,
      "no-ex-assign": 2,
      "no-extend-native": 2,
      "no-extra-parens": 0,
      "no-extra-semi": 2,
      "no-floating-decimal": 2,
      "no-func-assign": 2,
      "no-implied-eval": 2,
      "no-inner-declarations": 2,
      "no-mixed-requires": 0,
      "no-multi-spaces": [
        2,
        {
          "exceptions": {
            "VariableDeclarator": true
          }
        }
      ],
      "no-negated-in-lhs": 2,
      "no-new": 0,
      "no-obj-calls": 2,
      "no-sparse-arrays": 2,
      "no-trailing-spaces": 0,
      "no-underscore-dangle": 0,
      "no-unreachable": 2,
      "use-isnan": 2,
      "valid-typeof": 2
    }
  }
}
