{
  "name": "mopt",
  "version": "4.0.4",
  "description": "Babel plugin to statically optimize Mithril m() calls",
  "main": "./src/index.js",
  "author": "Pat Cavit <pcavit@arena.net>",
  "repository": "tivac/mithril-objectify",
  "license": "MIT",
  "keywords": [
    "mithril",
    "optimization",
    "ast",
    "babel-plugin"
  ],
  "dependencies": {
    "lodash.ismatchwith": "^4.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "eslint": "^3.12.2",
    "eslint-config-arenanet": "^3.1.0",
    "eslint-plugin-import": "^2.2.0",
    "istanbul": "^0.4.0",
    "mithril": "^1.0.0-rc.6",
    "mocha": "^3.2.0",
    "publish-please": "^2.2.0"
  },
  "scripts": {
    "test": "mocha",
    "posttest": "npm run lint",
    "lint": "echo LINTING GOES HERE",
    "cover": "istanbul test --print both ./node_modules/mocha/bin/_mocha",
    "preversion": "npm test",
    "publish-please": "publish-please",
    "prepublish": "publish-please guard"
  },
  "eslintConfig": {
    "extends": "arenanet",
    "plugins": [
      "import"
    ],
    "env": {
      "node": true,
      "mocha": true
    },
    "rules": {
      "import/no-unresolved": [
        "error",
        {
          "commonjs": true
        }
      ],
      "quotes": [
        "warn",
        "double",
        {
          "allowTemplateLiterals": true
        }
      ]
    }
  }
}
