{
  "name": "mopt",
  "version": "4.0.1",
  "description": "Babel plugin to optimize Mithril m() calls into simple JS objects",
  "main": "./src/index.js",
  "scripts": {
    "test": "npm run lint && npm run mocha",
    "lint": "eslint .",
    "cover": "istanbul test --print both ./node_modules/mocha/bin/_mocha",
    "mocha": "mocha",
    "preversion": "npm test",
    "publish-please": "publish-please",
    "prepublish": "publish-please guard"
  },
  "author": "Pat Cavit <pcavit@arena.net>",
  "repository": "tivac/mithril-objectify",
  "license": "MIT",
  "keywords": [
    "mithril",
    "optimization",
    "ast",
    "babel-plugin"
  ],
  "devDependencies": {
    "babel-core": "^6.8.0",
    "eslint": "^2.5.3",
    "eslint-config-arenanet": "^2.0.3",
    "istanbul": "^0.4.0",
    "lodash.assign": "^4.0.8",
    "mithril": "github:lhorie/mithril.js#rewrite",
    "mocha": "^2.4.5",
    "publish-please": "^2.1.4"
  },
  "publishConfig": {
    "tag": "rewrite"
  },
  "eslintConfig": {
    "extends": "arenanet",
    "env": {
      "node": true,
      "mocha": true
    }
  }
}
